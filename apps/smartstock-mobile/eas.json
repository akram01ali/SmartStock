{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "env": {
          "NODE_ENV": "development"
        }
      }
    },
    "preview-release": {
      "android": {
        "buildType": "apk",
        "env": {
          "NODE_ENV": "production"
        }
      }
    },
    "production": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
}
